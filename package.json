{
  "name": "abolish-browser",
  "version": "0.0.16",
  "description": "Browser version of abolish javascript validator",
  "main": "abolish.js",
  "repository": "https://github.com/trapcodeio/abolish-browser",
  "author": "trapcodeio",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm run bundle && npm run minify",
    "bundle": "browserify --external joi index.js -o abolish.js",
    "minify": "terser abolish.js --output='abolish.min.js'"
  },
  "dependencies": {},
  "devDependencies": {
    "abolish": "^4.5.3",
    "browserify": "^17.0.0",
    "terser": "^5.9.0"
  },
  "keywords": [
    "abolish",
    "abolish-browser",
    "object-validator-pro",
    "object-validation",
    "object-validator",
    "async-object-validator"
  ]
}
